{
  "name": "awm-server",
  "author": "Michael Pyle",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Awesome Workout Manager - Server",
  "keywords": [
    "node",
    "typescript",
    "postgresql",
    "react",
    "express"
  ],
  "main": "build/server.js",
  "scripts": {
    "build"    : "tsc",
    "clean"    : "rm -r .tsbuildinfo build/*",
    "lint"     : "tslint",
    "postbuild": "cp -r ./sql ./build",
    "rebuild"  : "npm run clean && npm run build",
    "start"    : "npm run build && node build/server.js",
    "test"     : "jest"
  },
  "dependencies": {
    "cors": "2.8.5",
    "date-fns": "2.16.1",
    "express": "4.17.1",
    "fp-ts": "2.9.3",
    "pg-promise": "10.9.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.9",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.27",
    "eslint": "7.20.0",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5"
  },
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "rootDir": "./src"
  }
}
