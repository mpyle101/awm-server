    "fp-ts": "2.9.3",
    "pg-promise": "10.9.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.9",
    "@types/express": "4.17.11",
    "@types/node": "14.14.21",
    "eslint": "7.18.0",
    "typescript": "4.1.3"
  }
}
